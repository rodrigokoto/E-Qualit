                                    <div class="form-group">
                                        <label for="form-auditoria-gestor" class="control-label">@Traducao.Auditoria.ResourceAuditoria.Auditoria_lbl_Gestor</label>
                                        <select id="form-auditoria-gestor" name="formAuditoriaGestor" class="form-control"
                                                data-msg-required="@Traducao.Auditoria.ResourceAuditoria.Auditoria_msg_required_Gestor">
                                            <option value="">@Traducao.Resource.Dropdown_name_select</option>
                                            @if (Model.FirstOrDefault() != null && Model.FirstOrDefault().Usuario != null)
                                            {
                                                <option value="@Model.FirstOrDefault().Usuario.IdUsuario" selected>@Model.FirstOrDefault().Usuario.NmCompleto</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="row">

                                    @if (listaPermissoes.Any(x => x.IdFuncao == (int)FuncoesAuditoria.CadastroDeNorma)
                                        || listaPermissoes.Any(x => x.IdFuncao == (int)FuncoesAuditoria.EdiçãoDeNorma)

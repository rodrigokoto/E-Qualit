@using ApplicationService.Entidade
@using ApplicationService.Enum

@model  List<Dominio.Entidade.Pai>

@{
    ViewBag.Title = "Auditoria";

    var listaPermissoes = new List<PermissoesApp>();
    listaPermissoes.AddRange(ViewBag.Permissoes);

}
@section Metas {
    <meta name="page" content="IndexAuditoria" />
}

@section scripts {
    <script src="@Url.Content("~/Content/assets/js/AuditoriaController.js")"></script>
    <script src="@Url.Content("~/Content/assets/js/FornecedoresController.js")"></script>
}

<div id="main">
    <div class="content-painel">
        <!-- hidden -->
        <input class="hidden" name="IdSite" value="@ViewBag.IdSite"
        <!-- Formulario Calendario -->
